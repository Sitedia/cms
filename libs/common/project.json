{
  "name": "libs/common",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/common/src",
  "projectType": "library",
  "tags": ["common"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["eslint ."],
        "cwd": "libs/common"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["jest --verbose"],
        "cwd": "libs/common"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["jest --coverage"],
        "cwd": "libs/common"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "libs/common"
      }
    }
  }
}
